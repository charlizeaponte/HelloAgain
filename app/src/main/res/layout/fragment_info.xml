<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context=".infoFragment">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Basic Info"
        android:textSize="30dp"
        android:textAlignment="center"
        />
    <Space
        android:layout_width="match_parent"
        android:layout_height="20sp"/>
    <EditText
        android:id="@+id/field_your_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/field_your_name_hint"
        />
    <Space
        android:layout_width="match_parent"
        android:layout_height="20sp"/>
    <EditText
        android:id="@+id/field_contact_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/field_contact_name_hint" />
    <Space
        android:layout_width="match_parent"
        android:layout_height="20sp"
        />
    <EditText
        android:id="@+id/field_relationship"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/field_relationship_hint"
        android:inputType="text"
        android:maxLines="1"
        />
    <Space
        android:layout_width="match_parent"
        android:layout_height="20sp"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <Button
            android:id="@+id/inBackbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Back"/>

        <Space
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" />
        <Button
            android:id="@+id/inForbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="68dp"
            android:text="Next" />
    </LinearLayout>





